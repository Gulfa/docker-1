pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
	sh 'echo $DOCKER_PASSWORD | docker login --username $DOCKER_USERNAME --password-stdin'
        //slackSend(channel: '#jenkins', message: 'Building.')
        sh '''ls
pwd
echo $WORKSPACE
ls $WORKSPACE
'''
      }
    }
  }
  environment {
    CI_PWD = '/var/lib/jenkins/workspace/dashboards/'
  }
}
